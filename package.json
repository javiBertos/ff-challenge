{
  "name": "ff-challenge",
  "description": "FinanceFox Challenge",
  "version": "0.0.1",
  "homepage": "https://github.com/javiBertos/ff-challenge",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Javier Bertos",
    "email": "yo@javierbertos.name",
    "url": "http://blog.javierbertos.name/"
  },
  "scripts": {
    "prestart": "npm install",
    "start": "http-server",
    "start-dev": "tsc -v && tsc && npm run build-css && concurrently \"http-server -s\" \"npm run tsc:w\" \"npm run build-css:w\"",
    "tsc": "tsc --prety",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "build-css": "node-sass scss --include-path scss --recursive --output css",
    "build-css:w": "nodemon -e scss -x \"npm run build-css\"",
    "build-assets": "tsc && npm run build-css"
  },
  "dependencies": {
    "backbone": "^1.3.3",
    "jquery": "^3.1.1",
    "http-server": "^0.9.0",
    "systemjs": "^0.19.42",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "node-sass": "^4.3.0",
    "nodemon": "^1.11.0",
    "typescript": "^2.1.5",
    "typings": "^2.1.0"
  }
}
